      "multi": false,
      "required": true
    }
  ],
  "registration_required": true,
  "subscription_required": true,
  "free_subscription": true,
  "service_homepage": "https://otx.alienvault.com/",
  "service_logo": {
    "path": "assets/OTX.png",
    "caption": "logo"
  },
  "screenshots": [
    {
      "path": "assets/long_report.png",
      "caption": "OTX Alienvault: long report"
    }
